datasource db {
	provider = "mysql"
	url = env("DATABASE_URL")
}
generator client {
	provider = "prisma-client-js"
	previewFeatures = ["fullTextSearch","fullTextIndex"]
}

model User {
    id        String    @id
    name      String
    email     String    @unique
    password  String
    phone     String
    admin     Boolean   @default(false)
}

model Artist {
    id          String  @id
    name        String
    description String
    bands       Band[]
    events      Event[]
}

model Band {
    id          String  @id
    name        String
    description String
    artists     Artist[]
